#평균 점수 구하기
#case1
A = [70, 60, 55, 75, 95, 90, 80, 80, 85, 100]
total = 0

for sum in A:
  total += sum
print(total/len(A))

#case2
A = [70, 60, 55, 75, 95, 90, 80, 80, 85, 100]
total = 0
i = 0

while i<len(A):
  total+=A[i]
  i+=1
print(total/len(A))

#case3
A = [70, 60, 55, 75, 95, 90, 80, 80, 85, 100]
total = 0
i = 0

while True:
  if i == len(A):
    print(total/len(A))
    break
  total+=A[i]
  i+=1
